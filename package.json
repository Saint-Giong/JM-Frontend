{
  "name": "jm-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n frontend,backend -c blue,green \"bun run dev:frontend\" \"bun run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd ../JM-Backend && docker compose up -d --build --force-recreate",
    "shutdown": "docker compose down",
    "build": "next build",
    "buildx": "bun ff && bun lint && bun run build",
    "start": "next start",
    "lint": "biome lint",
    "ff": "biome format --write",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@saint-giong/bamboo-ui": "^0.5.1",
    "@tanstack/react-query": "^5.90.12",
    "@vercel/analytics": "^1.6.1",
    "next": "^16.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
